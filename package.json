{
  "name": "risk-management",
  "version": "3.0.0",
  "description": "Template for the Learning Journey Building side-by-side extensions on SAP BTP",
  "author": "m.haug@sap.com",
  "license": "SAP SAMPLE CODE LICENSE",
  "repository": "https://github.com/SAP-samples/sap-learning-extension-suite",
  "dependencies": {
    "@sap-cloud-sdk/http-client": "2.15.0",
    "@sap/cds": "^7.2.0",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1.5.0",
    "@sap/ux-specification": "^1.120.3",
    "cds-plugin-ui5": "^0.6.13"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-risk-management": "cds watch --open riskmanagement/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-project1": "cds watch --open project1/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "requires": {
      "API_BUSINESS_PARTNER": {
        "kind": "odata-v2",
        "model": "srv/external/API_BUSINESS_PARTNER"
      }
    }
  },
  "sapux": [
    "app/risk-management",
    "app/project1",
    "app/risk-management"
